{
    "bindings":
    [
        {
            "entry": "net#0",
            "type" : "proxy",
            "kind": "server",
            "routes":
            {
                "exit": "app#0",
                "when":
                [
                    {
                        "transport": "stream",
                        "family": "inet4",
                        "destination":
                        {
                            "port": 443
                        },
                        "info":
                        {
                            "alpn": "echo"
                        }
                    }
                ]
            }
        }
    ]
}
